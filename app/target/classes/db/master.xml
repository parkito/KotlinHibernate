<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd"
        logicalFilePath="init.xml">
    <include file="classpath:/event/publisher/changelog/changelog.xml"/>

    <!-- Structure -->
    <include file="init/tables/_master.xml" relativeToChangelogFile="true"/>

    <!-- Constraints -->
    <include file="init/constraints/_master.xml" relativeToChangelogFile="true"/>

    <!-- Initial Data -->
    <include file="init/data/_master.xml" relativeToChangelogFile="true"/>

    <!-- Migration -->
    <include file="changelogs/_master.xml" relativeToChangelogFile="true"/>

</databaseChangeLog>
